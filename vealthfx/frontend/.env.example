# VealthX Frontend Environment Configuration
# Copy this file to .env.local and update with your values

# Contract address (update after deployment)
VITE_CONTRACT_ADDRESS=0x123...your_contract_address_here

# Aptos network configuration  
VITE_NODE_URL=https://fullnode.devnet.aptoslabs.com/v1
VITE_NETWORK=devnet

# Aptos Build Integration (CTRL+MOVE Hackathon)
VITE_APTOS_BUILD_API_KEY=your_api_key_here
VITE_GAS_STATION_ENABLED=true
VITE_INDEXER_ENDPOINT=https://api.devnet.aptoslabs.com/v1/graphql

# Real-time data feeds
VITE_GRAPHQL_ENDPOINT=https://api.devnet.aptoslabs.com/v1/graphql
VITE_WEBSOCKET_ENDPOINT=wss://api.devnet.aptoslabs.com/v1/stream

# Optional: Aptos Explorer base URL
VITE_EXPLORER_URL=https://explorer.aptoslabs.com

# Enhanced features
VITE_ENABLE_REAL_TIME_UPDATES=true
VITE_ENABLE_ORDER_BOOK=true
VITE_ENABLE_COPY_TRADING=true
